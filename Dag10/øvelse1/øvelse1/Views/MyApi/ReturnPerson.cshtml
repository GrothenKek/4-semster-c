
@{
    ViewBag.Title = "ReturnPerson";
}

<h2>ReturnPerson</h2>

<div class="jumbotron">
    <h1>Are you looking for a specific person?</h1>

    <div>
        <ul>
            @for(int i =0; i < ViewBag.Data.Count; i++)
            {
                <li>@ViewBag.Data[i]</li>

            }
        </ul>
    </div>


</div>

